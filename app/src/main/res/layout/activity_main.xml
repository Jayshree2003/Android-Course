<?xml version="1.0" encoding="utf-8"?>
<layout>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DessertLifeCycle.ModuleLifeCycleActivity">

    <Button
        android:id="@+id/btn_clrMyViews"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="Color My Views"
        app:layout_constraintBaseline_toBaselineOf="@+id/btn_dice"
        app:layout_constraintEnd_toStartOf="@+id/btn_dice"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_dice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="DiceRoller"
        app:layout_constraintBottom_toTopOf="@+id/btn_modulelifecycle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_clrMyViews"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_lifecycle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LifeCycles"
        app:layout_constraintBaseline_toBaselineOf="@+id/btn_modulelifecycle"
        app:layout_constraintEnd_toStartOf="@+id/btn_modulelifecycle"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_modulelifecycle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dessert LifeCycle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_lifecycle"
        app:layout_constraintTop_toBottomOf="@+id/btn_dice"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_architecture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Architecture"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_lifecycle" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>